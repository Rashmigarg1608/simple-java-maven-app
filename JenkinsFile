pipeline {
    agent  any

    
    stages {
        stage('Build') {
            steps {
                sh 'hi'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                }
            }
        }
        stage('Deliver') {
            steps {
              echo 'hii'
            }
        }
    }
    }
    
